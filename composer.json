{
    "name": "frame-php/installer",
    "type": "project",
    "license": "MIT",
    "description": "Main FramePHP installer",
    "authors": [
        {
            "name": "Hilkiah Makemo",
            "email": "hilkiah.makemo@skywest.com"
        }
    ],
    "require-dev": {
    },
    "require": {
        "frame-php/application": "dev-master",
        "frame-php/module-app": "dev-master",
        "frame-php/module-sys": "dev-master"
    },
    "scripts": {
        "post-create-project-cmd": [
            "frame project", 
            "composer update"
        ],
        "post-install-cmd": [
            "frame install"
        ],
        "post-update-cmd": [
            "frame update",
            "composer dump-autoload"
        ]
    },
    "bin": [
        "bin/frame"
    ],
    "minimum-stability": "dev"
}
