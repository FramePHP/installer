{
    "name": "frame-php/installer",
    "type": "project",
    "license": "MIT",
    "description": "Main FramePHP installer",
    "authors": [
        {
            "name": "Hilkiah Makemo",
            "email": "hilkiah.makemo@skywest.com"
        }
    ],
    "require-dev": {
    },
    "require": {
        "frame-php/application": "dev-master"
    },
    "scripts": {
        "moveFiles": [
            "@php -r \"require_once 'Install.php'; Install::PostCreateCMD();\""
        ],
        "trashFiles" : [            
        ],
        "post-create-project-cmd": [
            "@moveFiles", "@trashFiles"
        ],
        "post-install-cmd": [
            "@moveFiles", "@trashFiles"
        ],
        "post-update-cmd": [
            "@post-install-cmd"
        ]
    },
    "minimum-stability": "dev"
}
